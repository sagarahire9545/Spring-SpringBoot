<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="a1" class="com.collection.userdefine.Answer">
		<property name="aid" value="1"></property>

		<property name="ans" value="1.Java is a loop."></property>

	</bean>

	<bean id="q" class="com.collection.userdefine.Question">


		<property name="qid" value="101"></property>

		<property name="qname" value="What is Java?"></property>

		<property name="answer">

			<list>
				<ref bean="a1"></ref>


			</list>
		</property>

	</bean>

</beans>