<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="a1" class="com.collection.ActorMovie.Movie">
		<property name="mid" value="1"></property>

		<property name="mName" value="1.Shyamsing Roy"></property>

	</bean>

	<bean id="act" class="com.collection.ActorMovie.Actor">


		<property name="aid" value="101"></property>

		<property name="aname" value="Nani"></property>

		<property name="movie">

			<list>
				<ref bean="a1"></ref>


			</list>
		</property>

	</bean>

</beans>